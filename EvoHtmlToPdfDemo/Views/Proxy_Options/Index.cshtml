                                                                                                <td>
                                                                                                    <input name="usernameTextBox" type="text" id="usernameTextBox" style="width:80px;">
                                                                                                </td>
                                                                                                <td style="width: 10px"></td>
                                                                                                <td>
                                                                                                    Password:
                                                                                                </td>
