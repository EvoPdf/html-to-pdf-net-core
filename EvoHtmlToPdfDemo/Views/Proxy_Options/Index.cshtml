                                                                                                <td style="width: 5px"></td>
                                                                                                <td>
                                                                                                    <select name="proxyTypeDropDownList" id="proxyTypeDropDownList">
                                                                                                        <option selected="selected" value="None">None</option>
                                                                                                        <option value="Http">Http</option>
                                                                                                        <option value="Socks5">Socks5</option>
                                                                                                        <option value="HttpCaching">HttpCaching</option>

                                                                                                    </select>
                                                                                                </td>
                                                                                                <td style="width: 10px"></td>
                                                                                                <td>
                                                                                                    Host Name or IP:
                                                                                                </td>
                                                                                                <td style="width: 5px"></td>
                                                                                                <td>
                                                                                                    <input name="hostNameTextBox" type="text" id="hostNameTextBox" style="width:100px;">
                                                                                                </td>
                                                                                                <td style="width: 10px"></td>
                                                                                                <td>
                                                                                                    Port Number:
                                                                                                </td>
                                                                                                <td style="width: 5px"></td>
                                                                                                <td>
                                                                                                    <input name="portNumberTextBox" type="text" value="0" id="portNumberTextBox" style="width:50px;">
                                                                                                </td>
                                                                                            </tr>
                                                                                        </tbody>
                                                                                    </table>
